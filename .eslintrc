{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  },
  "rules": {
    "class-methods-use-this": "off",
    "function-paren-newline": "off"
  }
}
